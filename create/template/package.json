{
  "name": "@metaliq/template",
  "version": "1.0.0-beta.146",
  "description": "Update these values to suit your new solution",
  "type": "module",
  "main": "bin/models.js",
  "types": "bin/models.d.ts",
  "scripts": {
    "build": "pnpm build-md && pnpm build-codegen && pnpm build-api && pnpm build-app",
    "build-api": "metaliq build -f api/api-model apiModel",
    "build-app": "metaliq build",
    "build-codegen": "graphql-codegen",
    "build-md": "metadoc content src/gen/content",
    "compile-deps": "cd ../../core/metaliq && tsc && cd ../util && tsc && cd ../../policies/publication && tsc && cd ../../ui/metadoc && tsc",
    "pkg-prepare": "pnpm compile-deps && pnpm prepare",
    "prepare": "pnpm build-md && pnpm build-codegen",
    "start": "pnpm start-md & pnpm start-codegen & pnpm start-api & pnpm start-app",
    "start-api": "metaliq run -f api/api-model apiModel",
    "start-app": "metaliq run",
    "start-codegen": "graphql-codegen --watch",
    "start-md": "metadoc content src/gen/content --watch"
  },
  "author": "",
  "license": "",
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "dependencies": {
    "@metaliq/application": "workspace:*",
    "@metaliq/communication": "workspace:*",
    "@metaliq/forms": "workspace:*",
    "@metaliq/graphql-server": "workspace:*",
    "@metaliq/integration": "workspace:*",
    "@metaliq/modals": "workspace:*",
    "@metaliq/navigation": "workspace:*",
    "@metaliq/navigator": "workspace:*",
    "@metaliq/presentation": "workspace:*",
    "@metaliq/terminology": "workspace:*",
    "@metaliq/up": "workspace:*",
    "@metaliq/validation": "workspace:*",
    "@metaliq/web-page-app": "workspace:*",
    "bootstrap-icons": "1.10.5",
    "graphqlex": "0.18.0",
    "lit": "^3.1.4",
    "metaliq": "workspace:*"
  },
  "devDependencies": {
    "@graphql-codegen/add": "3.2.1",
    "@graphql-codegen/cli": "2.13.7",
    "@graphql-codegen/typescript": "2.8.0",
    "@graphql-codegen/typescript-operations": "2.5.5",
    "@graphql-codegen/typescript-resolvers": "2.7.5",
    "@metaliq/cli": "workspace:*",
    "@metaliq/metadoc": "workspace:*",
    "@metaliq/publication": "workspace:*",
    "@types/node": "^20.6.5",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "graphql": "^16.9.0",
    "graphqlex-codegen": "0.23.0",
    "typescript": "^5.2.2"
  },
  "typedocOptions": {
    "entryPoints": ["./src/*.ts"]
  }
}
